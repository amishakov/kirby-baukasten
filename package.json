{
  "name": "@coralic/kirby-baukasten",
  "version": "3.0.0",
  "description": "An opinionated, supercharged version of the Kirby CMS Plainkit",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "kirby": "env-cmd --use-shell \"php -S \\$KIRBY_DEV_HOSTNAME:\\$KIRBY_DEV_PORT -t public kirby/router.php\"",
    "dev": "shx touch src/.lock && concurrently -n \"php,vite\" \"pnpm:kirby\" \"vite\"",
    "build": "shx rm -f src/.lock && vite build"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fontaine": "^0.3.1",
    "postcss": "^8.4.20",
    "prettier": "2.8.1",
    "sass": "^1.57.1",
    "shx": "^0.3.4",
    "stylelint": "^14.16.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-full-reload": "^1.0.5"
  },
  "dependencies": {
    "tachyonjs": "^1.1.3",
    "vanilla-lazyload": "^17.8.3"
  }
}
